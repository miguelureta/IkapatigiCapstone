<body>


    <div class="bg-container">



        <input type="checkbox" id="check" />
        <label for="check">
            <i class="fa-solid fa-bars" id="okbutton"></i>
            <i class="fa-solid fa-x" id="cancelbutton"></i>

        </label>
        <div class="SideBar">
            <h1>IkapaTigi</h1>
            <ul>
                <li><a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index"> <i class="fa-solid fa-home"></i> Home </a> </li>
                <li><a class="navbar-brand" asp-area="" asp-controller="Diagnose" asp-action="Index"> <i class="fa-solid fa-stethoscope"></i> Diagnose </a> </li>
                <li><a class="navbar-brand" asp-area="" asp-controller="Diagnose" asp-action="Index"> <i class="fa-solid fa-book"></i> How To's </a> </li>
                <li> <a class="navbar-brand" asp-area="" asp-controller="Diagnose" asp-action="Index"> <i class="fa-solid fa-people-group"></i> About Us </a> </li>
                <li> <a class="navbar-brand" asp-area="" asp-controller="Diagnose" asp-action="Index"> <i class="fa-solid fa-tag"></i> Pricing </a> </li>
            </ul>
            <br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br />
            <ul>
                <li> <a class="navbar-brand" asp-area="" asp-page="/Index">  <i class="fa-solid fa-right-to-bracket"></i> Login </a> </li>
                <li> <a class="navbar-brand" asp-area="" asp-page="/Index">  <i class="fa-solid fa-file-pen"></i> Register </a> </li>

            </ul>


        </div>

        <div class="Logo">
            <img src="~/BackgroundImages/Logo.png" />
        </div>

        <br />  <br /> <br />



        <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2022 - IkapatigiCapstone - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>


